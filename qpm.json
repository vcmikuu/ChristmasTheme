{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "version": "0.1.0",
  "info": {
    "name": "Christmas",
    "id": "christmas",
    "version": "1.0.0",
    "url": null,
    "additionalData": {
      "overrideSoName": "libchristmas.so",
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": ["pwsh ./scripts/build.ps1 -clean"],
      "copy": [
        "pwsh ./scripts/copy.ps1 $0:?"
      ],
      "logcat": [
        "pwsh ./scripts/start-logging.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1",
        "pwsh ./scripts/createqmod.ps1"
      ],
      "restart": [
        "pwsh ./scripts/restart-game.ps1"
      ],
      "stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ],
      "tomb": [
        "pwsh ./scripts/pull-tombstone.ps1"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": ["./build", "./extern/libs"],
    "qmodOutput": "./Christmas.qmod"
  },
  "dependencies": [
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    }
  ]
}
